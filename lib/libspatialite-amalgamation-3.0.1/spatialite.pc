libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: spatialite
Description: Spatial SQL database engine based on SQLite
Version: 3.0.1
Libs: -L${libdir} -lspatialite -lm
Cflags: -I${includedir} -DSPATIALITE_AMALGAMATION=1
